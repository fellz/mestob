    },
    "projects": {
      "collection": "projects",
      "via": "participants"
    },
    "myteams": {
      "collection": "teams",
      "via": "leader"
    },
    "team_requests": {
      "collection": "teams",
      "via": "requests"
    },
    "inteams": {
      "collection": "teams",
